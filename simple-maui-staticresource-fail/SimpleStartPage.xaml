<?xml version="1.0" encoding="utf-8" ?>
<base:ContentPageBase
	x:Class="Nau.Simple.Maui.Staticresource.Fail.SimpleStartPage"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:base="clr-namespace:Nau.Simple.Maui.Staticresource.Fail"
	Title="SimpleStartPage">
	<VerticalStackLayout>
		<Label Style="{StaticResource SectionHeaderLabelStyle}" Text="Style applies OK." />
		<Label Text="Tap to go to List Page">
			<Label.GestureRecognizers>
				<TapGestureRecognizer Command="{Binding GoToListPage}" />
			</Label.GestureRecognizers>
		</Label>
	</VerticalStackLayout>
</base:ContentPageBase>